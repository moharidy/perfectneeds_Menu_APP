<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mohammed.Menu_App">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".MakiActivity"
            android:label="@string/category_maki"
            android:screenOrientation="landscape" />
        <activity
            android:name=".MakiSubActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".StartActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity_Arabic" android:screenOrientation="landscape"/>
        <activity android:name=".MakiActivity_Arabic" android:screenOrientation="landscape" />
        <activity android:name=".MakiSubActivity_Arabic" android:screenOrientation="landscape"/>

    </application>

</manifest>